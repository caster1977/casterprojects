  <B>if</B> (Str=<FONT color=#2a28bd>'1,06'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'1.06'</FONT>) <B>then</B> Result:= acLnWt106;
  <B>if</B> (Str=<FONT color=#2a28bd>'1,20'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'1.20'</FONT>) <B>then</B> Result:= acLnWt120;
