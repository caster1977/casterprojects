  <B>if</B> (Str=<FONT color=#2a28bd>'0,30'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'0.30'</FONT>) <B>then</B> Result:= acLnWt030;
  <B>if</B> (Str=<FONT color=#2a28bd>'0,35'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'0.35'</FONT>) <B>then</B> Result:= acLnWt035;
