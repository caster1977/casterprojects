     <B>or</B> (Str=<FONT color=#2a28bd>'по умолчанию'</FONT>) <B>then</B> Result:= acLnWtByLwDefault;
   Вернуться к оглавлению
  <B>if</B> (Str=<FONT color=#2a28bd>'0,05'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'0.05'</FONT>) <B>then</B> Result:= acLnWt005;
  <B>if</B> (Str=<FONT color=#2a28bd>'0,09'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'0.09'</FONT>) <B>then</B> Result:= acLnWt009;
