  <B>if</B> (Str=<FONT color=#2a28bd>'0,15'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'0.15'</FONT>) <B>then</B> Result:= acLnWt015;
  <B>if</B> (Str=<FONT color=#2a28bd>'0,18'</FONT>) <B>or</B> (Str=<FONT color=#2a28bd>'0.18'</FONT>) <B>then</B> Result:= acLnWt018;
